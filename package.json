{
    "name": "venbot",
    "version": "1.0.0",
    "description": "Discord Bot for the Vencord server",
    "main": "dist/index.js",
    "scripts": {
        "start": "pnpm sql:up & pnpm build && node . VENBOT",
        "build": "node build.mjs",
        "test": "pnpm lint && pnpm testTypes",
        "testTypes": "tsc --noEmit",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "pnpm lint --fix",
        "sql:shell": "sqlite3 data/db.sqlite3",
        "sql:up": "mkdir -p data && sqlite3 data/db.sqlite3 < sql/create.sql",
        "sql:down": "mkdir -p data && sqlite3 data/db.sqlite3 < sql/drop.sql",
        "sql:types": "kysely-codegen"
    },
    "keywords": [],
    "author": "Vendicated <vendicated+npm@riseup.net>",
    "dependencies": {
        "better-sqlite3": "^11.9.1",
        "canvas": "^2.11.2",
        "dotenv": "^16.4.5",
        "fastify": "^5.2.2",
        "kysely": "^0.27.6",
        "leven": "^4.0.0",
        "markdown-table": "^3.0.4",
        "nanoid": "^5.1.5",
        "oceanic.js": "1.11.2",
        "pako": "^2.1.0",
        "parse-duration": "^2.1.4",
        "source-map-support": "^0.5.21",
        "valibot": "^1.0.0",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.12",
        "@types/node": "^20.17.6",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/parser": "^7.7.0",
        "esbuild": "^0.20.2",
        "eslint": "^8.46.0",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "kysely-codegen": "^0.18.3",
        "pino-pretty": "^13.0.0",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    },
    "engines": {
        "node": ">=20"
    }
}
